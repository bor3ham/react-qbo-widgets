{
  "name": "react-qbo-widgets",
  "version": "1.0.6",
  "description": "Simple widget library with minimal dependencies to match QBO's theme ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-js": "./node_modules/webpack/bin/webpack.js",
    "build-css": "./node_modules/node-sass/bin/node-sass src/react-qbo-widgets.sass dist/react-qbo-widgets.css",
    "build": "npm run build-js; npm run build-css",
    "prepare": "cp src/qbo-variables.sass dist; npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bor3ham/react-qbo-widgets.git"
  },
  "keywords": [
    "react",
    "minimal",
    "widget",
    "library",
    "qbo",
    "quickbooks",
    "online"
  ],
  "author": "Michael Boreham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bor3ham/react-qbo-widgets/issues"
  },
  "homepage": "https://github.com/bor3ham/react-qbo-widgets#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.22.1",
    "react": "^15.4",
    "react-dom": "^15.4",
    "react-select": "^1.2.1",
    "react-virtualized-select": "^3.1.3"
  },
  "peerDependencies": {
    "react": "0.14.x || 15.x",
    "react-dom": "0.14.x || 15.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "node-sass": "^4.9.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
